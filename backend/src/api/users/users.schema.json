{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "User",
    "description": "The User",
    "required": [
        "email",
        "name",
        "user_name",
        "password"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of user"
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The users email. Must be unique. ",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "abcuser_i@null.adp"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The Users name ",
            "examples": [
                "Ron"
            ]
        },
        "user_name": {
            "$id": "#/properties/user_name",
            "type": "string",
            "title": "The Users user name. Must be unique. ",
            "examples": [
                "Roni_super"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The Users hashed password "
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the user"
            
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the user last updated"
        }
    },
    "additionalProperties": false
}